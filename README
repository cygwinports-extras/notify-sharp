  /usr/lib/monodoc/sources/notify-sharp-docs.source
  /usr/lib/monodoc/sources/notify-sharp-docs.tree
  /usr/lib/monodoc/sources/notify-sharp-docs.zip
  /usr/lib/pkgconfig/notify-sharp.pc
